<?xml version="1.0"?>
<RuleSet Name="ApiDesign Rules with default severity" Description="All ApiDesign Rules with default severity. Rules with IsEnabledByDefault = false or from a different category are disabled." ToolsVersion="15.0">
   <!-- ApiDesign Rules -->
   <Rules AnalyzerId="Roslyn.Diagnostics.Analyzers" RuleNamespace="Roslyn.Diagnostics.Analyzers">
      <Rule Id="RS0016" Action="Warning" />          <!-- Add public types and members to the declared API -->
      <Rule Id="RS0017" Action="Warning" />          <!-- Remove deleted types and members from the declared API -->
      <Rule Id="RS0022" Action="Warning" />          <!-- Constructor make noninheritable base class inheritable -->
      <Rule Id="RS0024" Action="Warning" />          <!-- The contents of the public API files are invalid -->
      <Rule Id="RS0025" Action="Warning" />          <!-- Do not duplicate symbols in public API files -->
      <Rule Id="RS0026" Action="Warning" />          <!-- Do not add multiple public overloads with optional parameters -->
      <Rule Id="RS0027" Action="Warning" />          <!-- Public API with optional parameter(s) should have the most parameters amongst its public overloads. -->
      <Rule Id="RS0030" Action="Warning" />          <!-- Do not used banned APIs -->
      <Rule Id="RS0031" Action="Warning" />          <!-- The list of banned symbols contains a duplicate -->
      <Rule Id="RS0035" Action="Error" />            <!-- External access to internal symbols outside the restricted namespace(s) is prohibited -->
      <Rule Id="RS0036" Action="Warning" />          <!-- Annotate nullability of public types and members in the declared API -->
      <Rule Id="RS0037" Action="Warning" />          <!-- Annotate nullability of types and members in the declared API -->
      <Rule Id="RS0041" Action="Warning" />          <!-- Public members should not use oblivious types -->
   </Rules>



   <!-- Other Rules -->
</RuleSet>
